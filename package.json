{
  "name": "auth",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.8.1",
    "@japa/preset-adonis": "^1.1.1",
    "@japa/runner": "^2.1.1",
    "@types/uuid": "^8.3.4",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "openapi-typescript": "^5.4.1",
    "pino-pretty": "^9.1.0",
    "prettier": "^2.7.1",
    "typescript": "~4.6.4",
    "youch": "^3.2.1",
    "youch-terminal": "^2.1.5"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.2",
    "@adonisjs/core": "^5.8.6",
    "@adonisjs/lucid": "^18.1.0",
    "@adonisjs/repl": "^3.1.11",
    "http-message-signatures": "file:../http-message-signatures",
    "jose": "^4.9.2",
    "luxon": "^3.0.3",
    "openapi": "file:../rafiki/packages/openapi",
    "openapi-types": "^12.0.2",
    "pg": "^8.8.0",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.0.11",
    "uuid": "^8.3.2"
  }
}
